// begin login
.login.login-with-news-feed(ng-controller='LoginCtrl')
  // begin news-feed
  .news-feed
    .news-image
      img(src='assets/images/bg-1.jpg', data-id='login-cover-image', alt='')
    .news-caption
      h4.caption-title
        // i.fa.fa-diamond.text-success
        // |  VuScreen
        img(src='assets/images/logo.png', width='55px', style='padding-right: 10px')
        | VuScreen
      p
        | .
  // end news-feed
  // begin right-content
  .right-content
    // begin login-header
    .login-header
      .brand
        // <span class="logo"></span> VuScreen
        img(src='assets/images/logo.png', width='55px', style='padding-right: 10px')
        | VuScreen
        small
          | Know more about VuScreen.
      .icon
        i.fa.fa-sign-in
    // end login-header
    // begin login-content
    .login-content
      form.margin-bottom-0(ng-submit='login(loginForm)', novalidate='')
        .alert.alert-danger.fade.in.m-b-15(ng-show='errors.other')
          strong Error!
          |                     {{ errors.other }}
          span.close(data-dismiss='alert') ×
        .form-group.m-b-15
          md-input-container
            input.form-control.input-lg(required='', type='email', ng-model='user.email', name='email', placeholder='Email Address', autocomplete='on')
            div(ng-messages='form.email.$error')
              div(ng-message='required') This is required.
        .form-group.m-b-15
          md-input-container
            input.form-control.input-lg(required='', type='password', ng-model='user.password', name='password', placeholder='Password', autocomplete='on')
            div(ng-messages='form.password.$error')
              div(ng-message='required') This is required.
        // .checkbox.m-b-30
          label
            input(type='checkbox', ng-model='chkRememberMe')
            |  Remember Me
        .checkbox.m-b-30
          label
